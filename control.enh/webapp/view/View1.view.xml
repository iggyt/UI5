<mvc:View
    controllerName="com.iggit.control.enh.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:unified="sap.ui.unified"
    xmlns:my="com.iggit.control.enh.control"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <VBox id="vbox-1" width="50%">
                <my:myCalendar
                    id="myCalendar-1"
                    finalizeDay="20250903"
                    legend="legend-1"
                    specialDays="{specialDays>/data}"
                />
                <my:myCalendarLegend
                    id="legend-1"
                    specialDays="{specialDays>/data}"
                    width="50%"
                />
            </VBox>
        </content>
    </Page>
</mvc:View>
